{
  "name": "divante-ltd/pimcore-mysql-object-search",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Similar to Pimcore's Advanced Object Search, but does not need Elasticsearch",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://www.divante.com",
  "authors": [
    {
      "name": "Korneliusz Kirsz",
      "email": "kkirsz@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Bruno Ramalho",
      "email": "bramalho@divante.pl",
      "role": "Developer"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "symfony/phpunit-bridge": "^4.2",
    "divante-ltd/pimcore-coding-standards": "^0.1",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.1",
    "block8/php-docblock-checker": "^1.10",
    "phploc/phploc": "^4.0.0",
    "codeception/codeception": "^3.0"
  },
  "require": {
    "pimcore/pimcore": ">=5.5.0"
  },
  "autoload": {
    "psr-4": {
      "DivanteLtd\\AdvancedSearchBundle\\": "src/AdvancedSearchBundle"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "DivanteLtd\\AdvancedSearchBundle\\AdvancedSearchBundle"
      ]
    }
  }
}
